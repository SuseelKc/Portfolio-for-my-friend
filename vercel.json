{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.4" }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/(.*\\.(?:css|js|png|jpg|jpeg|svg|gif|ico|woff2?|ttf))", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
